; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Admob",
"language": "GDScript",
"path": "res://admob-lib/admob.gd"
}, {
"base": "Node2D",
"class": "Ship",
"language": "GDScript",
"path": "res://Scenes/Classes/Ship.gd"
}, {
"base": "Area2D",
"class": "Shot",
"language": "GDScript",
"path": "res://Scenes/Classes/Shot.gd"
}, {
"base": "Node2D",
"class": "Tank",
"language": "GDScript",
"path": "res://Scenes/Classes/Tank.gd"
} ]
_global_script_class_icons={
"Admob": "res://admob-lib/icon.png",
"Ship": "",
"Shot": "",
"Tank": ""
}

[android]

modules="org/godotengine/godot/GodotAdMob,org/godotengine/godot/PlayGameServices"

[application]

config/name="Odyssey Lite"
run/main_scene="res://Scenes/Cenas/Menu.tscn"
boot_splash/image="res://icon_xtudio.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.152941, 0.0509804, 0.278431, 1 )
config/icon="res://Assets/launcher_icon/launcher_xxxhdpi.png"

[autoload]

Global="*res://Scenes/Classes/PlayGameService.gd"
GlobalVals="*res://Scenes/Classes/Global.gd"

[display]

window/size/width=540
window/size/height=1080
window/size/test_width=270
window/size/test_height=540
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

Tests={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":83,"unicode":0,"echo":false,"script":null)
 ]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

translations=PoolStringArray( "res://Assets/Localization/localization.en.translation", "res://Assets/Localization/localization.es.translation", "res://Assets/Localization/localization.pt.translation" )
locale_filter=[ 0, [ "en", "pt_BR" ] ]
translation_remaps={
"res://Assets/.important/CHANGELOG_EN.txt": PoolStringArray( "res://Assets/.important/CHANGELOG_PT.txt:pt_BR" ),
"res://Assets/.important/PATREON_APOIASE_EN.txt": PoolStringArray( "res://Assets/.important/PATREON_APOIASE_PT.txt:pt_BR" )
}

[rendering]

environment/default_environment="res://default_env.tres"
